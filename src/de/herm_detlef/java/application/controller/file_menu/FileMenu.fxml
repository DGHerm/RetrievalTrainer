<?import javafx.scene.input.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<Menu mnemonicParsing="false" text="%L10N_MenuBar_Menu_File" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <MenuItem mnemonicParsing="false" onAction="#onOpenFileChooser" text="Read Exercise Item List ...">
         <accelerator>
            <KeyCodeCombination alt="UP" code="O" control="UP" meta="UP" shift="UP" shortcut="DOWN" />
         </accelerator>
      </MenuItem>
      <SeparatorMenuItem mnemonicParsing="false" />
      <MenuItem fx:id="menuItemSave" disable="true" mnemonicParsing="false" text="Save Exercise Item List">
         <accelerator>
            <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="UP" shortcut="DOWN" />
         </accelerator>
      </MenuItem>
      <MenuItem fx:id="menuItemSaveAs" disable="true" mnemonicParsing="false" text="Save As..." />
   </items>
</Menu>
